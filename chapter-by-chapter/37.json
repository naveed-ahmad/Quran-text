{
  "total": 182,
  "verses": {
    "1": "وَالصّٰٓفّٰتِ صَفًّا  ۙ‏ ﴿﻿۱﻿﴾  ",
    "2": "فَالزّٰجِرٰتِ زَجۡرًا  ۙ‏ ﴿﻿۲﻿﴾  ",
    "3": "فَالتّٰلِيٰتِ ذِكۡرًا  ۙ‏ ﴿﻿۳﻿﴾  ",
    "4": "اِنَّ اِلٰهَكُمۡ لَوَاحِدٌ ؕ‏ ﴿﻿۴﻿﴾  ",
    "5": "رَبُّ السَّمٰوٰتِ وَالۡاَرۡضِ وَمَا بَيۡنَهُمَا وَرَبُّ الۡمَشَارِقِ ؕ‏ ﴿﻿۵﻿﴾  ",
    "6": "اِنَّا زَيَّنَّا السَّمَآءَ الدُّنۡيَا بِزِيۡنَةِ اۨلۡكَوَاكِبِۙ‏ ﴿﻿۶﻿﴾  ",
    "7": "وَحِفۡظًا  مِّنۡ كُلِّ شَيۡطٰنٍ مَّارِدٍ‌ۚ‏ ﴿﻿۷﻿﴾  ",
    "8": "لَّا يَسَّمَّعُوۡنَ اِلَى الۡمَلَاِ الۡاَعۡلٰى وَيُقۡذَفُوۡنَ مِنۡ كُلِّ جَانِبٍۖ ‏ ﴿﻿۸﻿﴾  ",
    "9": "دُحُوۡرًا  وَّلَهُمۡ عَذَابٌ وَّاصِبٌ  ۙ‏ ﴿﻿۹﻿﴾  ",
    "10": "اِلَّا مَنۡ خَطِفَ  الۡخَطۡفَةَ فَاَتۡبَعَهٗ شِهَابٌ ثَاقِبٌ‏ ﴿﻿۱۰﻿﴾  ",
    "11": "فَاسۡتَفۡتِهِمۡ اَهُمۡ اَشَدُّ خَلۡقًا اَمۡ  مَّنۡ خَلَقۡنَاؕ اِنَّا خَلَقۡنٰهُمۡ مِّنۡ طِيۡنٍ لَّازِبٍ‏ ﴿﻿۱۱﻿﴾  ",
    "12": "بَلۡ عَجِبۡتَ وَيَسۡخَرُوۡنَ‏ ﴿﻿۱۲﻿﴾  ",
    "13": "وَاِذَا ذُكِّرُوۡا لَا يَذۡكُرُوۡنَ‏ ﴿﻿۱۳﻿﴾  ",
    "14": "وَاِذَا رَاَوۡا اٰيَةً يَّسۡتَسۡخِرُوۡنَ‏ ﴿﻿۱۴﻿﴾  ",
    "15": "وَقَالُوۡۤا اِنۡ  هٰذَاۤ اِلَّا سِحۡرٌ مُّبِيۡنٌ‌ ۖ‌ۚ‏ ﴿﻿۱۵﻿﴾  ",
    "16": "ءَاِذَا مِتۡنَا وَكُـنَّا تُرَابًا وَّعِظَامًا ءَاِنَّا لَمَبۡعُوۡثُوۡنَۙ‏ ﴿﻿۱۶﻿﴾  ",
    "17": "اَوَاٰبَآؤُنَا الۡاَوَّلُوۡنَؕ‏ ﴿﻿۱۷﻿﴾  ",
    "18": "قُلۡ نَعَمۡ وَاَنۡـتُمۡ دٰخِرُوۡنَ‌ۚ‏ ﴿﻿۱۸﻿﴾  ",
    "19": "فَاِنَّمَا هِىَ زَجۡرَةٌ  وَّاحِدَةٌ فَاِذَا هُمۡ يَنۡظُرُوۡنَ‏ ﴿﻿۱۹﻿﴾  ",
    "20": "وَقَالُوۡا يٰوَيۡلَنَا هٰذَا يَوۡمُ الدِّيۡنِ‏ ﴿﻿۲۰﻿﴾  ",
    "21": "هٰذَا  يَوۡمُ الۡفَصۡلِ الَّذِىۡ كُنۡتُمۡ بِهٖ تُكَذِّبُوۡنَ‏ ﴿﻿۲۱﻿﴾  ",
    "22": "اُحۡشُرُوا الَّذِيۡنَ ظَلَمُوۡا وَاَزۡوَاجَهُمۡ وَمَا كَانُوۡا يَعۡبُدُوۡنَۙ‏ ﴿﻿۲۲﻿﴾  ",
    "23": "مِنۡ دُوۡنِ اللّٰهِ فَاهۡدُوۡهُمۡ اِلٰى  صِرَاطِ الۡجَحِيۡمِ‏ ﴿﻿۲۳﻿﴾  ",
    "24": "وَقِفُوۡهُمۡ‌ اِنَّهُمۡ مَّسْـُٔـوۡلُوۡنَۙ‏ ﴿﻿۲۴﻿﴾  ",
    "25": "مَا لَـكُمۡ لَا تَنَاصَرُوۡنَ‏ ﴿﻿۲۵﻿﴾  ",
    "26": "بَلۡ هُمُ الۡيَوۡمَ مُسۡتَسۡلِمُوۡنَ‏ ﴿﻿۲۶﻿﴾  ",
    "27": "وَاَقۡبَلَ بَعۡضُهُمۡ عَلٰى بَعۡضٍ يَّتَسَآءَلُوۡنَ‏ ﴿﻿۲۷﻿﴾  ",
    "28": "قَالُوۡۤا اِنَّكُمۡ كُنۡتُمۡ تَاۡتُوۡنَنَا عَنِ الۡيَمِيۡنِ‏ ﴿﻿۲۸﻿﴾  ",
    "29": "قَالُوۡا بَلْ لَّمۡ تَكُوۡنُوۡا مُؤۡمِنِيۡنَ‌ۚ‏ ﴿﻿۲۹﻿﴾  ",
    "30": "وَمَا كَانَ لَنَا عَلَيۡكُمۡ مِّنۡ سُلۡطٰنِ‌ۚ بَلۡ كُنۡتُمۡ قَوۡمًا طٰغِيۡنَ‏ ﴿﻿۳۰﻿﴾  ",
    "31": "فَحَقَّ عَلَيۡنَا  قَوۡلُ رَبِّنَآ   ۖ اِنَّا لَذَآٮِٕقُوۡنَ‏ ﴿﻿۳۱﻿﴾  ",
    "32": "فَاَغۡوَيۡنٰكُمۡ اِنَّا كُنَّا غٰوِيۡنَ‏ ﴿﻿۳۲﻿﴾  ",
    "33": "فَاِنَّهُمۡ يَوۡمَٮِٕذٍ فِى الۡعَذَابِ مُشۡتَرِكُوۡنَ‏ ﴿﻿۳۳﻿﴾  ",
    "34": "اِنَّا كَذٰلِكَ نَفۡعَلُ بِالۡمُجۡرِمِيۡنَ‏ ﴿﻿۳۴﻿﴾  ",
    "35": "اِنَّهُمۡ كَانُوۡۤا اِذَا قِيۡلَ لَهُمۡ لَاۤ اِلٰهَ اِلَّا اللّٰهُۙ يَسۡتَكۡبِرُوۡنَۙ‏ ﴿﻿۳۵﻿﴾  ",
    "36": "وَيَقُوۡلُوۡنَ اَٮِٕنَّا لَتٰرِكُوۡۤا اٰلِهَـتِنَا لِشَاعِرٍ مَّجۡـنُوۡنٍ  ؕ‏ ﴿﻿۳۶﻿﴾  ",
    "37": "بَلۡ جَآءَ بِالۡحَقِّ وَصَدَّقَ  الۡمُرۡسَلِيۡنَ‏ ﴿﻿۳۷﻿﴾  ",
    "38": "اِنَّكُمۡ لَذَآٮِٕقُوا الۡعَذَابِ الۡاَلِيۡمِ‌ۚ‏ ﴿﻿۳۸﻿﴾  ",
    "39": "وَمَا تُجۡزَوۡنَ اِلَّا مَا  كُنۡتُمۡ تَعۡمَلُوۡنَۙ‏ ﴿﻿۳۹﻿﴾  ",
    "40": "اِلَّا عِبَادَ اللّٰهِ الۡمُخۡلَصِيۡنَ‏ ﴿﻿۴۰﻿﴾  ",
    "41": "اُولٰٓٮِٕكَ لَهُمۡ رِزۡقٌ  مَّعۡلُوۡمٌۙ‏ ﴿﻿۴۱﻿﴾  ",
    "42": "فَوَاكِهُ‌ۚ وَهُمۡ مُّكۡرَمُوۡنَۙ‏ ﴿﻿۴۲﻿﴾  ",
    "43": "فِىۡ جَنّٰتِ النَّعِيۡمِۙ‏ ﴿﻿۴۳﻿﴾  ",
    "44": "عَلٰى سُرُرٍ  مُّتَقٰبِلِيۡنَ‏ ﴿﻿۴۴﻿﴾  ",
    "45": "يُطَافُ عَلَيۡهِمۡ بِكَاۡسٍ مِّنۡ مَّعِيۡنٍۢ ۙ‏ ﴿﻿۴۵﻿﴾  ",
    "46": "بَيۡضَآءَ لَذَّةٍ  لِّلشّٰرِبِيۡنَ‌ ۖ‌ۚ‏ ﴿﻿۴۶﻿﴾  ",
    "47": "لَا فِيۡهَا غَوۡلٌ وَّلَا هُمۡ عَنۡهَا يُنۡزَفُوۡنَ‏ ﴿﻿۴۷﻿﴾  ",
    "48": "وَعِنۡدَهُمۡ قٰصِرٰتُ  الطَّرۡفِ عِيۡنٌۙ‏ ﴿﻿۴۸﻿﴾  ",
    "49": "كَاَنَّهُنَّ بَيۡضٌ مَّكۡنُوۡنٌ‏ ﴿﻿۴۹﻿﴾  ",
    "50": "فَاَقۡبَلَ بَعۡضُهُمۡ عَلٰى بَعۡضٍ  يَّتَسَآءَلُوۡنَ‏ ﴿﻿۵۰﻿﴾  ",
    "51": "قَالَ قَآٮِٕلٌ مِّنۡهُمۡ اِنِّىۡ كَانَ لِىۡ قَرِيۡنٌۙ‏ ﴿﻿۵۱﻿﴾  ",
    "52": "يَقُوۡلُ اَءِ نَّكَ لَمِنَ الۡمُصَدِّقِيۡنَ‏ ﴿﻿۵۲﻿﴾  ",
    "53": "ءَاِذَا مِتۡنَا وَكُنَّا تُرَابًا وَّعِظَامًا  ءَاِنَّا لَمَدِيۡنُوۡنَ‏ ﴿﻿۵۳﻿﴾  ",
    "54": "قَالَ هَلۡ اَنۡتُمۡ مُّطَّلِعُوۡنَ‏ ﴿﻿۵۴﻿﴾  ",
    "55": "فَاطَّلَعَ  فَرَاٰهُ فِىۡ سَوَآءِ الۡجَحِيۡمِ‏ ﴿﻿۵۵﻿﴾  ",
    "56": "قَالَ تَاللّٰهِ اِنۡ كِدْتَّ لَـتُرۡدِيۡنِۙ‏ ﴿﻿۵۶﻿﴾  ",
    "57": "وَلَوۡلَا نِعۡمَةُ رَبِّىۡ لَـكُنۡتُ مِنَ الۡمُحۡضَرِيۡنَ‏ ﴿﻿۵۷﻿﴾  ",
    "58": "اَفَمَا نَحۡنُ  بِمَيِّتِيۡنَۙ‏ ﴿﻿۵۸﻿﴾  ",
    "59": "اِلَّا مَوۡتَتَـنَا الۡاُوۡلٰى وَمَا نَحۡنُ بِمُعَذَّبِيۡنَ‏ ﴿﻿۵۹﻿﴾  ",
    "60": "اِنَّ  هٰذَا لَهُوَ الۡفَوۡزُ الۡعَظِيۡمُ‏ ﴿﻿۶۰﻿﴾  ",
    "61": "لِمِثۡلِ هٰذَا فَلۡيَعۡمَلِ الۡعٰمِلُوۡنَ‏ ﴿﻿۶۱﻿﴾  ",
    "62": "اَذٰ لِكَ خَيۡرٌ نُّزُلًا اَمۡ شَجَرَةُ الزَّقُّوۡمِ‏ ﴿﻿۶۲﻿﴾  ",
    "63": "اِنَّا جَعَلۡنٰهَا فِتۡنَةً لِّلظّٰلِمِيۡنَ‏ ﴿﻿۶۳﻿﴾  ",
    "64": "اِنَّهَا شَجَرَةٌ تَخۡرُجُ فِىۡۤ اَصۡلِ الۡجَحِيۡمِۙ‏ ﴿﻿۶۴﻿﴾  ",
    "65": "طَلۡعُهَا كَاَنَّهٗ رُءُوۡسُ  الشَّيٰطِيۡنِ‏ ﴿﻿۶۵﻿﴾  ",
    "66": "فَاِنَّهُمۡ لَاٰكِلُوۡنَ مِنۡهَا فَمٰلِــُٔــوۡنَ مِنۡهَا الۡبُطُوۡنَ  ؕ‏ ﴿﻿۶۶﻿﴾  ",
    "67": "ثُمَّ اِنَّ لَهُمۡ عَلَيۡهَا لَشَوۡبًا مِّنۡ حَمِيۡمٍ‌ۚ‏ ﴿﻿۶۷﻿﴾  ",
    "68": "ثُمَّ اِنَّ مَرۡجِعَهُمۡ لَا۟اِلَى  الۡجَحِيۡمِ‏ ﴿﻿۶۸﻿﴾  ",
    "69": "اِنَّهُمۡ اَلۡفَوۡا اٰبَآءَهُمۡ ضَآلِّيۡنَۙ‏ ﴿﻿۶۹﻿﴾  ",
    "70": "فَهُمۡ عَلٰٓى اٰثٰرِهِمۡ  يُهۡرَعُوۡنَ‏ ﴿﻿۷۰﻿﴾  ",
    "71": "وَلَـقَدۡ ضَلَّ قَبۡلَهُمۡ اَكۡثَرُ الۡاَوَّلِيۡنَۙ‏ ﴿﻿۷۱﻿﴾  ",
    "72": "وَلَقَدۡ  اَرۡسَلۡنَا فِيۡهِمۡ مُّنۡذِرِيۡنَ‏ ﴿﻿۷۲﻿﴾  ",
    "73": "فَانْظُرۡ كَيۡفَ كَانَ عَاقِبَةُ  الۡمُنۡذَرِيۡنَۙ‏ ﴿﻿۷۳﻿﴾  ",
    "74": "اِلَّا عِبَادَ اللّٰهِ الۡمُخۡلَصِيۡنَ‏ ﴿﻿۷۴﻿﴾  ",
    "75": "وَلَقَدۡ نَادٰٮنَا نُوۡحٌ فَلَنِعۡمَ الۡمُجِيۡبُوۡنَ   ۖ‏ ﴿﻿۷۵﻿﴾  ",
    "76": "وَنَجَّيۡنٰهُ وَاَهۡلَهٗ مِنَ الۡكَرۡبِ الۡعَظِيۡمِ   ۖ‏ ﴿﻿۷۶﻿﴾  ",
    "77": "وَجَعَلۡنَا ذُرِّيَّتَهٗ هُمُ الۡبٰقِيۡنَ   ۖ‏ ﴿﻿۷۷﻿﴾  ",
    "78": "وَتَرَكۡنَا عَلَيۡهِ فِى الۡاٰخِرِيۡنَ   ۖ‏ ﴿﻿۷۸﻿﴾  ",
    "79": "سَلٰمٌ عَلٰى نُوۡحٍ فِى الۡعٰلَمِيۡنَ‏ ﴿﻿۷۹﻿﴾  ",
    "80": "اِنَّا كَذٰلِكَ نَجۡزِى الۡمُحۡسِنِيۡنَ‏ ﴿﻿۸۰﻿﴾  ",
    "81": "اِنَّهٗ مِنۡ عِبَادِنَا الۡمُؤۡمِنِيۡنَ‏ ﴿﻿۸۱﻿﴾  ",
    "82": "ثُمَّ اَغۡرَقۡنَا الۡاٰخَرِيۡنَ‏ ﴿﻿۸۲﻿﴾  ",
    "83": "وَاِنَّ مِنۡ شِيۡعَتِهٖ لَاِبۡرٰهِيۡمَ‌ۘ‏ ﴿﻿۸۳﻿﴾  ",
    "84": "اِذۡ جَآءَ رَبَّهٗ بِقَلۡبٍ سَلِيۡمٍ‏ ﴿﻿۸۴﻿﴾  ",
    "85": "اِذۡ قَالَ لِاَبِيۡهِ وَقَوۡمِهٖ مَاذَا تَعۡبُدُوۡنَ‌ۚ‏ ﴿﻿۸۵﻿﴾  ",
    "86": "اَٮِٕفۡكًا اٰلِهَةً دُوۡنَ اللّٰهِ  تُرِيۡدُوۡنَؕ‏ ﴿﻿۸۶﻿﴾  ",
    "87": "فَمَا ظَنُّكُمۡ بِرَبِّ الۡعٰلَمِيۡنَ‏ ﴿﻿۸۷﻿﴾  ",
    "88": "فَنَظَرَ نَظۡرَةً فِى النُّجُوۡمِۙ‏ ﴿﻿۸۸﻿﴾  ",
    "89": "فَقَالَ اِنِّىۡ سَقِيۡمٌ‏ ﴿﻿۸۹﻿﴾  ",
    "90": "فَتَوَلَّوۡا عَنۡهُ مُدۡبِرِيۡنَ‏ ﴿﻿۹۰﻿﴾  ",
    "91": "فَرَاغَ اِلٰٓى اٰلِهَتِهِمۡ  فَقَالَ اَلَا تَاۡكُلُوۡنَ‌ۚ‏ ﴿﻿۹۱﻿﴾  ",
    "92": "مَا لَـكُمۡ لَا تَنۡطِقُوۡنَ‏ ﴿﻿۹۲﻿﴾  ",
    "93": "فَرَاغَ عَلَيۡهِمۡ  ضَرۡبًۢا بِالۡيَمِيۡنِ‏ ﴿﻿۹۳﻿﴾  ",
    "94": "فَاَقۡبَلُوۡۤا اِلَيۡهِ يَزِفُّوۡنَ‏ ﴿﻿۹۴﻿﴾  ",
    "95": "قَالَ اَتَعۡبُدُوۡنَ مَا  تَنۡحِتُوۡنَۙ‏ ﴿﻿۹۵﻿﴾  ",
    "96": "وَاللّٰهُ خَلَقَكُمۡ وَمَا تَعۡمَلُوۡنَ‏ ﴿﻿۹۶﻿﴾  ",
    "97": "قَالُوا ابۡنُوۡا لَهٗ بُنۡيَانًا فَاَلۡقُوۡهُ فِى الۡجَحِيۡمِ‏ ﴿﻿۹۷﻿﴾  ",
    "98": "فَاَرَادُوۡا بِهٖ كَيۡدًا فَجَعَلۡنٰهُمُ الۡاَسۡفَلِيۡنَ‏ ﴿﻿۹۸﻿﴾  ",
    "99": "وَقَالَ اِنِّىۡ ذَاهِبٌ اِلٰى رَبِّىۡ سَيَهۡدِيۡنِ‏ ﴿﻿۹۹﻿﴾  ",
    "100": "رَبِّ هَبۡ لِىۡ مِنَ  الصّٰلِحِيۡنَ‏ ﴿﻿۱۰۰﻿﴾  ",
    "101": "فَبَشَّرۡنٰهُ بِغُلٰمٍ حَلِيۡمٍ‏ ﴿﻿۱۰۱﻿﴾  ",
    "102": "فَلَمَّا بَلَغَ مَعَهُ السَّعۡىَ قَالَ يٰبُنَىَّ اِنِّىۡۤ اَرٰى فِى الۡمَنَامِ اَنِّىۡۤ اَذۡبَحُكَ فَانْظُرۡ مَاذَا تَرٰى‌ؕ قَالَ يٰۤاَبَتِ افۡعَلۡ مَا تُؤۡمَرُ‌ سَتَجِدُنِىۡۤ اِنۡ شَآءَ اللّٰهُ مِنَ الصّٰبِرِيۡنَ‏ ﴿﻿۱۰۲﻿﴾  ",
    "103": "فَلَمَّاۤ اَسۡلَمَا وَتَلَّهٗ لِلۡجَبِيۡنِ‌ۚ‏ ﴿﻿۱۰۳﻿﴾  ",
    "104": "وَنَادَيۡنٰهُ اَنۡ يّٰۤاِبۡرٰهِيۡمُۙ‏ ﴿﻿۱۰۴﻿﴾  ",
    "105": "قَدۡ صَدَّقۡتَ الرُّءۡيَا ‌ ‌ۚ اِنَّا كَذٰلِكَ نَجۡزِى الۡمُحۡسِنِيۡنَ‏ ﴿﻿۱۰۵﻿﴾  ",
    "106": "اِنَّ  هٰذَا لَهُوَ الۡبَلٰٓؤُا الۡمُبِيۡنُ‏ ﴿﻿۱۰۶﻿﴾  ",
    "107": "وَفَدَيۡنٰهُ بِذِبۡحٍ عَظِيۡمٍ‏ ﴿﻿۱۰۷﻿﴾  ",
    "108": "وَتَرَكۡنَا عَلَيۡهِ فِى الۡاٰخِرِيۡنَ‌ۖ‏ ﴿﻿۱۰۸﻿﴾  ",
    "109": "سَلٰمٌ عَلٰٓى اِبۡرٰهِيۡمَ‏ ﴿﻿۱۰۹﻿﴾  ",
    "110": "كَذٰلِكَ نَجۡزِى الۡمُحۡسِنِيۡنَ‏ ﴿﻿۱۱۰﻿﴾  ",
    "111": "اِنَّهٗ مِنۡ عِبَادِنَا الۡمُؤۡمِنِيۡنَ‏ ﴿﻿۱۱۱﻿﴾  ",
    "112": "وَبَشَّرۡنٰهُ  بِاِسۡحٰقَ نَبِيًّا مِّنَ الصّٰلِحِيۡنَ‏ ﴿﻿۱۱۲﻿﴾  ",
    "113": "وَبٰرَكۡنَا عَلَيۡهِ وَعَلٰٓى اِسۡحٰقَ‌ؕ وَ مِنۡ ذُرِّيَّتِهِمَا مُحۡسِنٌ وَّظَالِمٌ لِّنَفۡسِهٖ مُبِيۡنٌ‌‏ ﴿﻿۱۱۳﻿﴾  ",
    "114": "وَلَقَدۡ مَنَنَّا عَلٰى مُوۡسٰى وَهٰرُوۡنَ‌ۚ‏ ﴿﻿۱۱۴﻿﴾  ",
    "115": "وَنَجَّيۡنٰهُمَا وَقَوۡمَهُمَا مِنَ الۡكَرۡبِ الۡعَظِيۡمِ‌ۚ‏ ﴿﻿۱۱۵﻿﴾  ",
    "116": "وَنَصَرۡنٰهُمۡ فَكَانُوۡا هُمُ الۡغٰلِبِيۡنَ‌ۚ‏ ﴿﻿۱۱۶﻿﴾  ",
    "117": "وَاٰتَيۡنٰهُمَا الۡكِتٰبَ الۡمُسۡتَبِيۡنَ‌ۚ‏ ﴿﻿۱۱۷﻿﴾  ",
    "118": "وَهَدَيۡنٰهُمَا الصِّرَاطَ الۡمُسۡتَقِيۡمَ‌ۚ‏ ﴿﻿۱۱۸﻿﴾  ",
    "119": "وَتَرَكۡنَا عَلَيۡهِمَا فِى الۡاٰخِرِيۡنَۙ‏ ﴿﻿۱۱۹﻿﴾  ",
    "120": "سَلٰمٌ عَلٰى مُوۡسٰى وَهٰرُوۡنَ‏ ﴿﻿۱۲۰﻿﴾  ",
    "121": "اِنَّا كَذٰلِكَ نَجۡزِى الۡمُحۡسِنِيۡنَ‏ ﴿﻿۱۲۱﻿﴾  ",
    "122": "اِنَّهُمَا مِنۡ عِبَادِنَا الۡمُؤۡمِنِيۡنَ‏ ﴿﻿۱۲۲﻿﴾  ",
    "123": "وَاِنَّ اِلۡيَاسَ  لَمِنَ الۡمُرۡسَلِيۡنَؕ‏ ﴿﻿۱۲۳﻿﴾  ",
    "124": "اِذۡ قَالَ لِقَوۡمِهٖۤ اَلَا تَتَّقُوۡنَ‏ ﴿﻿۱۲۴﻿﴾  ",
    "125": "اَتَدۡعُوۡنَ بَعۡلًا وَّتَذَرُوۡنَ اَحۡسَنَ الۡخٰلِقِيۡنَۙ‏ ﴿﻿۱۲۵﻿﴾  ",
    "126": "اللّٰهَ رَبَّكُمۡ وَرَبَّ اٰبَآٮِٕكُمُ  الۡاَوَّلِيۡنَ‏ ﴿﻿۱۲۶﻿﴾  ",
    "127": "فَكَذَّبُوۡهُ فَاِنَّهُمۡ لَمُحۡضَرُوۡنَۙ‏ ﴿﻿۱۲۷﻿﴾  ",
    "128": "اِلَّا عِبَادَ اللّٰهِ  الۡمُخۡلَصِيۡنَ‏ ﴿﻿۱۲۸﻿﴾  ",
    "129": "وَتَرَكۡنَا عَلَيۡهِ فِى الۡاٰخِرِيۡنَۙ‏ ﴿﻿۱۲۹﻿﴾  ",
    "130": "سَلٰمٌ عَلٰٓى  اِلۡ يَاسِيۡنَ‏ ﴿﻿۱۳۰﻿﴾  ",
    "131": "اِنَّا كَذٰلِكَ نَجۡزِى الۡمُحۡسِنِيۡنَ‏ ﴿﻿۱۳۱﻿﴾  ",
    "132": "اِنَّهٗ مِنۡ  عِبَادِنَا الۡمُؤۡمِنِيۡنَ‏ ﴿﻿۱۳۲﻿﴾  ",
    "133": "وَاِنَّ لُوۡطًا لَّمِنَ الۡمُرۡسَلِيۡنَؕ‏ ﴿﻿۱۳۳﻿﴾  ",
    "134": "اِذۡ نَجَّيۡنٰهُ وَاَهۡلَهٗۤ اَجۡمَعِيۡنَۙ‏ ﴿﻿۱۳۴﻿﴾  ",
    "135": "اِلَّا عَجُوۡزًا فِى الۡغٰبِرِيۡنَ‏ ﴿﻿۱۳۵﻿﴾  ",
    "136": "ثُمَّ  دَمَّرۡنَا الۡاٰخَرِيۡنَ‏ ﴿﻿۱۳۶﻿﴾  ",
    "137": "وَاِنَّكُمۡ لَتَمُرُّوۡنَ عَلَيۡهِمۡ مُّصۡبِحِيۡنَۙ‏ ﴿﻿۱۳۷﻿﴾  ",
    "138": "وَبِالَّيۡلِ‌ؕ  اَفَلَا تَعۡقِلُوۡنَ‌‏ ﴿﻿۱۳۸﻿﴾  ",
    "139": "وَاِنَّ يُوۡنُسَ لَمِنَ الۡمُرۡسَلِيۡنَؕ‏ ﴿﻿۱۳۹﻿﴾  ",
    "140": "اِذۡ اَبَقَ اِلَى  الۡفُلۡكِ الۡمَشۡحُوۡنِۙ‏ ﴿﻿۱۴۰﻿﴾  ",
    "141": "فَسَاهَمَ فَكَانَ مِنَ الۡمُدۡحَضِيۡنَ‌ۚ‏ ﴿﻿۱۴۱﻿﴾  ",
    "142": "فَالۡتَقَمَهُ الۡحُوۡتُ وَهُوَ مُلِيۡمٌ‏ ﴿﻿۱۴۲﻿﴾  ",
    "143": "فَلَوۡلَاۤ اَنَّهٗ كَانَ مِنَ الۡمُسَبِّحِيۡنَۙ‏ ﴿﻿۱۴۳﻿﴾  ",
    "144": "لَلَبِثَ فِىۡ بَطۡنِهٖۤ اِلٰى يَوۡمِ يُبۡعَثُوۡنَ‌ۚ‏ ﴿﻿۱۴۴﻿﴾  ",
    "145": "فَنَبَذۡنٰهُ بِالۡعَرَآءِ وَهُوَ  سَقِيۡمٌ‌ۚ‏ ﴿﻿۱۴۵﻿﴾  ",
    "146": "وَاَنۡۢبَتۡنَا عَلَيۡهِ شَجَرَةً مِّنۡ يَّقۡطِيۡنٍ‌ۚ‏ ﴿﻿۱۴۶﻿﴾  ",
    "147": "وَاَرۡسَلۡنٰهُ اِلٰى  مِائَةِ اَلۡفٍ اَوۡ يَزِيۡدُوۡنَ‌ۚ‏ ﴿﻿۱۴۷﻿﴾  ",
    "148": "فَاٰمَنُوۡا فَمَتَّعۡنٰهُمۡ اِلٰى حِيۡنٍؕ‏ ﴿﻿۱۴۸﻿﴾  ",
    "149": "فَاسۡتَفۡتِهِمۡ اَلِرَبِّكَ الۡبَنَاتُ وَلَهُمُ الۡبَنُوۡنَۙ‏ ﴿﻿۱۴۹﻿﴾  ",
    "150": "اَمۡ خَلَقۡنَا الۡمَلٰٓٮِٕكَةَ  اِنَاثًا وَّهُمۡ شٰهِدُوۡنَ‏ ﴿﻿۱۵۰﻿﴾  ",
    "151": "اَلَاۤ اِنَّهُمۡ مِّنۡ اِفۡكِهِمۡ لَيَقُوۡلُوۡنَۙ‏ ﴿﻿۱۵۱﻿﴾  ",
    "152": "وَلَدَ اللّٰهُۙ وَاِنَّهُمۡ لَـكٰذِبُوۡنَ‏ ﴿﻿۱۵۲﻿﴾  ",
    "153": "اَصۡطَفَى الۡبَنَاتِ عَلَى الۡبَنِيۡنَؕ‏ ﴿﻿۱۵۳﻿﴾  ",
    "154": "مَا لَـكُمۡ كَيۡفَ تَحۡكُمُوۡنَ‏ ﴿﻿۱۵۴﻿﴾  ",
    "155": "اَفَلَا تَذَكَّرُوۡنَ‌ۚ‏ ﴿﻿۱۵۵﻿﴾  ",
    "156": "اَمۡ لَـكُمۡ سُلۡطٰنٌ  مُّبِيۡنٌۙ‏ ﴿﻿۱۵۶﻿﴾  ",
    "157": "فَاۡتُوۡا بِكِتٰبِكُمۡ اِنۡ كُنۡتُمۡ صٰدِقِيۡنَ‏ ﴿﻿۱۵۷﻿﴾  ",
    "158": "وَجَعَلُوۡا بَيۡنَهٗ  وَبَيۡنَ الۡجِنَّةِ نَسَبًا ؕ‌ وَلَقَدۡ عَلِمَتِ الۡجِنَّةُ اِنَّهُمۡ لَمُحۡضَرُوۡنَۙ‏ ﴿﻿۱۵۸﻿﴾  ",
    "159": "سُبۡحٰنَ اللّٰهِ عَمَّا يَصِفُوۡنَۙ‏ ﴿﻿۱۵۹﻿﴾  ",
    "160": "اِلَّا عِبَادَ اللّٰهِ الۡمُخۡلَصِيۡنَ‏ ﴿﻿۱۶۰﻿﴾  ",
    "161": "فَاِنَّكُمۡ وَمَا تَعۡبُدُوۡنَۙ‏ ﴿﻿۱۶۱﻿﴾  ",
    "162": "مَاۤ اَنۡـتُمۡ عَلَيۡهِ بِفٰتِنِيۡنَۙ‏ ﴿﻿۱۶۲﻿﴾  ",
    "163": "اِلَّا مَنۡ هُوَ  صَالِ الۡجَحِيۡمِ‏ ﴿﻿۱۶۳﻿﴾  ",
    "164": "وَمَا مِنَّاۤ اِلَّا لَهٗ مَقَامٌ مَّعۡلُوۡمٌۙ‏ ﴿﻿۱۶۴﻿﴾  ",
    "165": "وَّاِنَّا لَـنَحۡنُ  الصَّآفُّوۡنَ‌ۚ‏ ﴿﻿۱۶۵﻿﴾  ",
    "166": "وَاِنَّا لَـنَحۡنُ الۡمُسَبِّحُوۡنَ‏ ﴿﻿۱۶۶﻿﴾  ",
    "167": "وَاِنۡ كَانُوۡا لَيَقُوۡلُوۡنَۙ‏ ﴿﻿۱۶۷﻿﴾  ",
    "168": "لَوۡ اَنَّ عِنۡدَنَا ذِكۡرًا مِّنَ الۡاَوَّلِيۡنَۙ‏ ﴿﻿۱۶۸﻿﴾  ",
    "169": "لَـكُنَّا عِبَادَ اللّٰهِ الۡمُخۡلَصِيۡنَ‏ ﴿﻿۱۶۹﻿﴾  ",
    "170": "فَكَفَرُوۡا بِهٖ‌ فَسَوۡفَ يَعۡلَمُوۡنَ‏ ﴿﻿۱۷۰﻿﴾  ",
    "171": "وَلَقَدۡ سَبَقَتۡ كَلِمَتُنَا لِعِبَادِنَا  الۡمُرۡسَلِيۡنَ ‌ۖ‌ۚ‏ ﴿﻿۱۷۱﻿﴾  ",
    "172": "اِنَّهُمۡ لَهُمُ الۡمَنۡصُوۡرُوۡنَ ‏ ﴿﻿۱۷۲﻿﴾  ",
    "173": "وَاِنَّ جُنۡدَنَا لَهُمُ  الۡغٰلِبُوۡنَ‏ ﴿﻿۱۷۳﻿﴾  ",
    "174": "فَتَوَلَّ عَنۡهُمۡ حَتّٰى حِيۡنٍۙ‏ ﴿﻿۱۷۴﻿﴾  ",
    "175": "وَاَبۡصِرۡهُمۡ فَسَوۡفَ يُبۡصِرُوۡنَ‏ ﴿﻿۱۷۵﻿﴾  ",
    "176": "اَفَبِعَذَابِنَا يَسۡتَعۡجِلُوۡنَ‏ ﴿﻿۱۷۶﻿﴾  ",
    "177": "فَاِذَا نَزَلَ بِسَاحَتِهِمۡ فَسَآءَ صَبَاحُ  الۡمُنۡذَرِيۡنَ‏ ﴿﻿۱۷۷﻿﴾  ",
    "178": "وَتَوَلَّ عَنۡهُمۡ حَتّٰى حِيۡنٍۙ‏ ﴿﻿۱۷۸﻿﴾  ",
    "179": "وَّاَبۡصِرۡ فَسَوۡفَ يُبۡصِرُوۡنَ‏ ﴿﻿۱۷۹﻿﴾  ",
    "180": "سُبۡحٰنَ رَبِّكَ رَبِّ الۡعِزَّةِ عَمَّا يَصِفُوۡنَ‌ۚ‏ ﴿﻿۱۸۰﻿﴾  ",
    "181": "وَسَلٰمٌ عَلَى الۡمُرۡسَلِيۡنَ‌ۚ‏ ﴿﻿۱۸۱﻿﴾  ",
    "182": "وَالۡحَمۡدُ لِلّٰهِ رَبِّ الۡعٰلَمِيۡنَ‌‏ ﴿﻿۱۸۲﻿﴾  "
  }
}